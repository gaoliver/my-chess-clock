{
	"env": {
		"es2021": true
	},
	"extends": [
		"plugin:react/recommended",
		"airbnb",
		"prettier"
	],
	"globals": {
		"Atomics": "readonly"
	},
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 2020,
		"sourceType": "module"
	},
	"plugins": [
		"react",
		"@typescript-eslint"
	],
	"rules": {
		"accessor-pairs": "warn",
		"arrow-parens": "warn",
		"arrow-spacing": "warn",
		"camelcase": "warn",
		"jsx-quotes": "warn",
		"quotes": "warn",
		"indent": "warn",
		"semi": "warn"
	}
}